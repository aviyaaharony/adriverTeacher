<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffe98f"
    android:backgroundTint="#ffe98f">

    <Button
        android:id="@+id/setLessons"
        android:layout_width="117dp"
        android:layout_height="105dp"
        android:layout_marginStart="166dp"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="310dp"
        android:backgroundTint="#ffbd59"
        android:radius="0dp"
        android:text="שיעורים שנקבעו"
        android:textSize="22sp"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/studentList" />

    <Button
        android:id="@+id/studentList"
        android:layout_width="117dp"
        android:layout_height="105dp"
        android:layout_marginStart="166dp"
        android:layout_marginTop="349dp"
        android:layout_marginEnd="165dp"
        android:backgroundTint="#ffbd59"
        android:radius="0dp"
        android:text="רשימת תלמידים"
        android:textSize="22sp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>